{
  "name": "react-native-event",
  "version": "0.2.14",
  "description": "React context for subscribing to all react native interaction events",
  "keywords": [
    "react-native",
    "react",
    "native",
    "outside",
    "press",
    "touch",
    "typescript"
  ],
  "repository": "kmalakoff/react-native-event",
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "clean": "node -e \"const dir = require('path').join(require('homedir-polyfill')(), '.im'); try {require('fs').rmdirSync(dir, {recursive: true})} catch(err) {}\"",
    "deploy": "tsds deploy",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:browser --config wtr.config.mjs",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/react": "^18.3.1",
    "@web/dev-server-import-maps": "*",
    "react": "^18.3.1",
    "react-native": "*",
    "react-native-web": "*",
    "react-test-renderer": "^18.3.1",
    "tsds-web-test-runner": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">=16"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
